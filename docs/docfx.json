{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "All/bin/**/*.dll"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "separatePages",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Monadic",
      "_appName": "Monadic",
      "_appFooter": null
    },
    "sitemap": {
      "baseUrl": "https://bfren.github.io/monadic",
      "priority": 0.5,
      "changefreq": "daily"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}